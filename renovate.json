{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "baseBranches": [
    "main"
  ],
  "ignoreDeps": [
    "@nuxt/image",
    "@vitest/coverage-v8",
    "vitest"
  ],
  "packageRules": [
    {
      "groupName": "docs",
      "matchPackageNames": [
        "@nuxt/content"
      ],
      "matchPackagePatterns": [
        "^@nuxthq/",
        "^@nuxt/ui"
      ]
    }
  ]
}
