{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>nuxt/renovate-config-nuxt"
  ],
  "baseBranchPatterns": [
    "main"
  ],
  "packageRules": [
    {
      "groupName": "ipx",
      "matchPackageNames": [
        "ipx",
        "sharp"
      ]
    },
    {
      "groupName": "docs",
      "matchPackageNames": [
        "@nuxt/content",
        "/^@nuxthq//",
        "/^@nuxt/ui/"
      ]
    }
  ]
}
