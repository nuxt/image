{
  "name": "@nuxt/image",
  "version": "0.4.17",
  "description": "Nuxt Image Module",
  "repository": "nuxt/image",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist",
    "vetur"
  ],
  "scripts": {
    "build": "siroc build && mkdist --src src/runtime --dist dist/runtime -d --ext js",
    "dev": "yarn nuxt playground",
    "docs:build": "cd docs && nuxt generate",
    "docs:dev": "yarn nuxt dev docs",
    "lint": "eslint --ext .ts --ext .vue .",
    "prepublishOnly": "yarn build",
    "release": "standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && yarn jest --forceExit",
    "test:e2e": "jest test/e2e --forceExit",
    "test:unit": "jest test/unit --forceExit"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "fs-extra": "^10.0.0",
    "hasha": "^5.2.2",
    "image-meta": "^0.0.1",
    "ipx": "^0.6.1",
    "is-https": "^4.0.0",
    "lru-cache": "^6.0.0",
    "node-fetch": "^2.6.1",
    "p-limit": "^3.1.0",
    "rc9": "^1.2.0",
    "requrl": "^3.0.2",
    "semver": "^7.3.5",
    "ufo": "^0.7.5",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@cyrilf/vue-dat-gui": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/fs-extra": "latest",
    "@types/jest": "latest",
    "@types/lru-cache": "latest",
    "@types/node-fetch": "latest",
    "@types/semver": "^7.3.6",
    "@vue/test-utils": "latest",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "jsdom-global": "latest",
    "mkdist": "latest",
    "nuxt": "^2.15.6",
    "playwright": "latest",
    "siroc": "latest",
    "standard-version": "latest",
    "ts-loader": "^8",
    "typescript": "latest",
    "vue-jest": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
