{
  "name": "@nuxt/image",
  "version": "2.0.0-alpha.1",
  "description": "Nuxt Image Module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/image.git"
  },
  "homepage": "https://image.nuxt.com",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/module.js"
  },
  "main": "./dist/module.js",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:generate": "nuxt generate playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare docs",
    "docs:build": "pnpm run --filter docs build",
    "docs:dev": "pnpm run --filter docs dev",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm build && changelogen --release --push && npm publish",
    "test": "nuxt prepare playground && pnpm vitest run --coverage",
    "test:types": "vue-tsc --noEmit && nuxt typecheck playground && nuxt typecheck example"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.7",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "h3": "^1.15.3",
    "image-meta": "^0.2.1",
    "knitwork": "^1.2.0",
    "ohash": "^2.0.11",
    "pathe": "^2.0.3",
    "std-env": "^3.9.0",
    "ufo": "^1.6.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "1.7.1",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "3.17.7",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^22.16.5",
    "@types/semver": "^7.7.0",
    "@unhead/vue": "^2.0.12",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "9.32.0",
    "happy-dom": "^18.0.1",
    "installed-check": "^9.3.0",
    "ipx": "^3.0.3",
    "jiti": "2.5.1",
    "knip": "^5.62.0",
    "nitropack": "^2.12.4",
    "nuxt": "^3.17.7",
    "ofetch": "^1.4.1",
    "pkg-pr-new": "^0.0.54",
    "playwright-core": "^1.54.1",
    "semver": "^7.7.2",
    "tinyexec": "^1.0.1",
    "tinyglobby": "^0.2.14",
    "typescript": "5.8.3",
    "unbuild": "^3.6.0",
    "vitest": "^3.2.4",
    "vitest-environment-nuxt": "^1.0.1",
    "vue": "3.5.18",
    "vue-tsc": "^3.0.4"
  },
  "optionalDependencies": {
    "ipx": "^3.0.3"
  },
  "packageManager": "pnpm@10.13.1",
  "resolutions": {
    "@nuxt/image": "workspace:*",
    "@nuxt/schema": "3.17.7",
    "@types/node": "^22.16.5",
    "vue": "3.5.18"
  },
  "engines": {
    "node": ">=18.20.6"
  }
}
