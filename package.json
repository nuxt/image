{
  "name": "@nuxt/image",
  "version": "0.0.3",
  "description": "Nuxt Image Module",
  "repository": "nuxt/image",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "templates",
    "types"
  ],
  "scripts": {
    "build": "yarn clean && tsc && tsc -p tsconfig.runtime.json",
    "postbuild": "copyfiles -u 1 src/**/*.css dist",
    "clean": "rimraf dist",
    "dev": "yarn nuxt-ts playground",
    "lint": "eslint --ext .ts --ext .vue .",
    "release": "yarn test && standard-version && yarn build && git push --follow-tags && npm publish",
    "test": "yarn lint && yarn test:unit && yarn test:e2e",
    "test:e2e": "jest test/e2e --forceExit",
    "test:unit": "jest test/unit --forceExit"
  },
  "dependencies": {
    "hasha": "^5.2.1",
    "ipx": "^0.3.2",
    "node-fetch": "^2.6.1",
    "upath": "^1.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "latest",
    "@babel/preset-typescript": "latest",
    "@nuxt/test-utils": "^0.0.1",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/jest": "latest",
    "@vue/test-utils": "^1.1.0",
    "babel-eslint": "latest",
    "copyfiles": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "nuxt-edge": "latest",
    "playwright": "^1.4.2",
    "rimraf": "latest",
    "standard-version": "latest",
    "typescript": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
