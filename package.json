{
  "name": "@nuxt/image",
  "version": "1.3.0",
  "description": "Nuxt Image Module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/image.git"
  },
  "homepage": "https://image.nuxt.com",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "docs:build": "cd docs && nuxt generate",
    "docs:dev": "pnpm nuxt dev docs",
    "lint": "eslint --ext .ts --ext .vue .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm build && changelogen --release --push && npm publish",
    "test": "nuxi prepare playground && pnpm vitest run",
    "test:types": "vue-tsc --noEmit && nuxi typecheck playground && nuxi typecheck example"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.1",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "h3": "^1.10.1",
    "image-meta": "^0.2.0",
    "node-fetch-native": "^1.6.2",
    "ohash": "^1.1.3",
    "pathe": "^1.1.2",
    "std-env": "^3.7.0",
    "ufo": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/image": "link:",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "changelogen": "^0.5.5",
    "eslint": "8.56.0",
    "globby": "^14.0.1",
    "happy-dom": "^13.3.8",
    "ipx": "^3.0.1",
    "jiti": "1.21.0",
    "nuxt": "^3.10.1",
    "playwright-core": "^1.41.2",
    "typescript": "5.3.3",
    "vitest": "^1.2.2",
    "vitest-environment-nuxt": "^1.0.0",
    "vue": "3.4.19",
    "vue-tsc": "^1.8.27"
  },
  "optionalDependencies": {
    "ipx": "^3.0.1"
  },
  "packageManager": "pnpm@8.15.2",
  "resolutions": {
    "@nuxt/schema": "3.10.1",
    "@nuxt/image-edge": "link:.",
    "@nuxt/image": "link:.",
    "vue": "3.3.13"
  },
  "engines": {
    "node": "^14.16.0 || >=16.11.0"
  }
}
