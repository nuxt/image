{
  "name": "@nuxt/image",
  "version": "1.0.0-rc.1",
  "description": "Nuxt Image Module",
  "repository": "nuxt/image",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare",
    "docs:build": "cd docs && nuxt generate",
    "docs:dev": "pnpm nuxt dev docs",
    "lint": "eslint --ext .ts --ext .vue .",
    "prepack": "pnpm build",
    "release": "pnpm run prerelease",
    "prerelease": "pnpm test && changelogen --release --prerelease=rc --push && npm publish",
    "test": "nuxi prepare playground && pnpm vitest run",
    "test:types": "vue-tsc --noEmit && nuxi typecheck playground && nuxi typecheck example"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.0",
    "consola": "^3.2.3",
    "defu": "^6.1.2",
    "h3": "^1.8.0",
    "image-meta": "^0.1.1",
    "node-fetch-native": "^1.4.0",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "std-env": "^3.4.3",
    "ufo": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/image": "link:.",
    "@nuxt/module-builder": "0.4.0",
    "@nuxt/test-utils": "^3.7.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.17.11",
    "@vitest/coverage-v8": "0.33.0",
    "@vue/test-utils": "^2.4.1",
    "changelogen": "^0.5.5",
    "eslint": "8.48.0",
    "globby": "^13.2.2",
    "happy-dom": "^10.11.0",
    "ipx": "1.2.0",
    "jiti": "1.19.3",
    "nuxt": "^3.7.0",
    "nuxt-vitest": "^0.10.2",
    "playwright": "^1.37.1",
    "typescript": "5.2.2",
    "vitest": "0.33.0",
    "vue-tsc": "^1.8.8"
  },
  "optionalDependencies": {
    "ipx": "^1.2.0"
  },
  "packageManager": "pnpm@8.6.12",
  "resolutions": {
    "@nuxt/image-edge": "link:.",
    "@nuxt/image": "link:.",
    "micromark": "3.1.0"
  },
  "engines": {
    "node": "^14.16.0 || >=16.11.0"
  }
}
