{
  "name": "@nuxt/image",
  "version": "1.0.0",
  "description": "Nuxt Image Module",
  "repository": "nuxt/image",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "docs:build": "cd docs && nuxt generate",
    "docs:dev": "pnpm nuxt dev docs",
    "lint": "eslint --ext .ts --ext .vue .",
    "prepack": "pnpm build",
    "release": "pnpm test && pnpm build && changelogen --release --push && npm publish",
    "test": "nuxi prepare playground && pnpm vitest run",
    "test:types": "vue-tsc --noEmit && nuxi typecheck playground && nuxi typecheck example"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.4",
    "consola": "^3.2.3",
    "defu": "^6.1.2",
    "h3": "^1.8.2",
    "image-meta": "^0.2.0",
    "node-fetch-native": "^1.4.0",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "std-env": "^3.4.3",
    "ufo": "^1.3.1"
  },
  "devDependencies": {
    "@nuxt/image": "link:",
    "@nuxt/module-builder": "0.5.2",
    "@nuxt/test-utils": "^3.7.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^20.8.7",
    "@vitest/coverage-v8": "0.34.4",
    "@vue/test-utils": "^2.4.1",
    "changelogen": "^0.5.5",
    "eslint": "8.51.0",
    "globby": "^13.2.2",
    "happy-dom": "^12.9.1",
    "ipx": "^2.0.0",
    "jiti": "1.20.0",
    "nuxt": "^3.7.4",
    "nuxt-vitest": "^0.11.0",
    "playwright": "^1.39.0",
    "typescript": "5.2.2",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.19"
  },
  "optionalDependencies": {
    "ipx": "^2.0.0"
  },
  "packageManager": "pnpm@8.9.2",
  "resolutions": {
    "@nuxt/image-edge": "link:.",
    "@nuxt/image": "link:."
  },
  "engines": {
    "node": "^14.16.0 || >=16.11.0"
  }
}